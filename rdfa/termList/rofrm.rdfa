<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    reg: http://metadataregistry.org/uri/profile/regap/
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofrm/T1002">
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/6962"></div>
    <div property="skos:definition" xml:lang="en" content="The content is not expected to be revised after initial release."></div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/rofrm">
        <div property="dc:title" content="ROF Revision Mode"></div>
      </div>
    </div>
    <div property="skos:prefLabel" xml:lang="en" content="not applicable"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofrm/T1001">
    <div property="skos:definition" xml:lang="en" content="Revision through the release of notices of errors, corrections, addenda, etc."></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/6961"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofrm"></div>
    <div property="skos:prefLabel" xml:lang="en" content="correction"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofrm/T1003">
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofrm"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/6963"></div>
    <div property="skos:prefLabel" xml:lang="en" content="substitution"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:definition" xml:lang="en" content="Revision through the release of pages, chapters, parts, volumes, etc., directly to replace previously released counterparts, including the replacement of the entire resource with a new version."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofrm/T1004">
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/6964"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofrm"></div>
    <div property="skos:prefLabel" xml:lang="en" content="transformation"></div>
    <div property="skos:definition" xml:lang="en" content="Revision through the incorporation of changes (including deletions) directly into the previously released content."></div>
  </div>
</div>