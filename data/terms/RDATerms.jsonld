            "api": "http://api.metadataregistry.org/concepts/8518",
            "inScheme": "http://rdaregistry.info/termList/RDATerms",
            "status": "Published",
            "prefLabel": {
                "en": "adaptation",
                "it": "adattamento",
                "fr": "adaptation",
