                                                                                },"@type":"Property","domain":{"@id":"http://rdaregistry.info/Elements/c/C10006", "lexicalAlias":"http://rdaregistry.info/Elements/c/Expression.en", "label":"expression"},"subPropertyOf":[{"@id":"http://rdaregistry.info/Elements/e/P20205", "lexicalAlias":"http://rdaregistry.info/Elements/e/relatedExpressionOfExpression.en", "label":"has related expression of expression"}],"lexicalAlias":"http://rdaregistry.info/Elements/e/precededByExpression.en","instructionNumber":"24.5","hasSubproperty":[{"@id":"http://rdaregistry.info/Elements/e/P20137", "lexicalAlias":"http://rdaregistry.info/Elements/e/absorptionOfExpression.en", "label":"is absorption of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20199", "lexicalAlias":"http://rdaregistry.info/Elements/e/replacementOfExpression.en", "label":"is replacement of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20149", "lexicalAlias":"http://rdaregistry.info/Elements/e/continuationOfExpression.en", "label":"is continuation of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20201", "lexicalAlias":"http://rdaregistry.info/Elements/e/mergerOfExpression.en", "label":"is merger of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20138", "lexicalAlias":"http://rdaregistry.info/Elements/e/absorptionInPartOfExpression.en", "label":"is absorption in part of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20198", "lexicalAlias":"http://rdaregistry.info/Elements/e/replacementInPartOfExpression.en", "label":"is replacement in part of expression"},{"@id":"http://rdaregistry.info/Elements/e/P20168", "lexicalAlias":"http://rdaregistry.info/Elements/e/separatedFromExpression.en", "label":"is separated from expression"},{"@id":"http://rdaregistry.info/Elements/e/P20334", "lexicalAlias":"http://rdaregistry.info/Elements/e/prequelExpression.en", "label":"has prequel expression"},{"@id":"http://rdaregistry.info/Elements/e/P