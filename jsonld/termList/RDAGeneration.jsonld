        {
            "@id": "http://rdaregistry.info/termList/RDAGeneration/1003",
            "@type": "Concept",
            "altLabel": {
                "en": "duplicate negative",
                "vi": "bản sao âm",
                "fr": [
                    "contretype négatif",
