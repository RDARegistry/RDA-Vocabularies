        {
            "@id": "http://rdaregistry.info/termList/RDATypeOfBinding/1003",
            "prefLabel": {
                "en": "spiral binding",
                "fi": "kierrenidonta",
                "nl": "spiraalband"
            },
            "note": {
                "en": ["Outer covers of the volume may be made of a rigid or flexible material, and may be attached directly to the coil or threaded by the coil"],
                "fi": ["Niteen kannet voivat olla tehty joustamattomasta tai joustavasta materiaalista, ja ne voivat olla kiinnitetyt suoraan tai pujoteltuna kierteeseen."],
                "nl": ["De kaft van de band kan gemaakt zijn van stug of flexibel materiaal, en kan direct verbonden zijn aan de spoel of geregen door de spoel."]
            },
            "ToolkitDefinition": {
                "en": "A type of binding that uses a continuous wire or plastic coil that is threaded through holes along the edge of each sheet.",
                "fi": "Sidonnan tyyppi, joka käyttää yhtenäistä metalli- ja muovikierrettä, joka pujotetaan arkkien reunassa olevien reikien läpi.",
                "nl": "Een type bindwijze dat een doorlopende draad- of plastic spoel gebruikt die geregen is door gaten aan de zijkant van elk vel."
            },
            "ToolkitLabel": {
                "en": "spiral binding",
                "fi": "kierrenidonta",
                "nl": "spiraalband"
            },
            "altLabel": {
                "en": ["coil binding"]
            },
            "inScheme": "http://rdaregistry.info/termList/RDATypeOfBinding",
            "status": {
                "@id": "http://metadataregistry.org/uri/RegStatus/1001",
                "lexicalAlias": "http://metadataregistry.org/uri/RegStatus/Published.en",
                "label": "Published"
            },
            "notation": {"en": "1003"}
