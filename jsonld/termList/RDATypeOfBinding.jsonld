            "@id": "http://rdaregistry.info/termList/RDATypeOfBinding",
            "@type": "ConceptScheme",
            "title": {"en": "RDA Type Of Binding"},
            "description": {"en": "Concepts for a method used to bind a published or unpublished manifestation."},
            "prefix": "rdatb",
            "issued": "2018-04-04",
            "modified": "2021-04-20",
            "versionInfo": "v4.0.6",
