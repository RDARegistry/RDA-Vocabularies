  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has revision requirement</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasRevisionRequirement.en"/>
