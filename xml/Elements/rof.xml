  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10002"/>
</rdf:Description>

<!-- Class: Qualified content category -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10001">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10007"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  
  <rdfs:label xml:lang="en">Qualified content category</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedContentCategory.en"/>
  <skos:definition xml:lang="en">A resource category that qualifies a base content category.</skos:definition>
  <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10005"/>
</rdf:Description>


<!-- Properties -->

<!-- Property: has applied material -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10001">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has applied material</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasAppliedMaterial.en"/>
  <skos:definition xml:lang="en">A material applied to the base material for purposes of infixing the content of the resource.</skos:definition>
  <skos:scopeNote xml:lang="en">Applies to resources conveying content on a physical carrier. Resources conveying content by means of broadcasting or online transmission will have an AppliedMaterial value not applicable.</skos:scopeNote>
</rdf:Description>

<!-- Property: has base material -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10007">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has base material</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasBaseMaterial.en"/>
  <skos:definition xml:lang="en">An underlying physical material on or in which the content of a resource is stored.</skos:definition>
  <skos:scopeNote xml:lang="en">Applies to resources conveying content on a physical carrier. Resources conveying content by means of broadcasting or online transmission will have a BaseMaterial value not applicable.</skos:scopeNote>
</rdf:Description>

<!-- Property: has capture method -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10017">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has capture method</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasCaptureMethod.en"/>
  <skos:definition xml:lang="en">A process used to capture the content of a resource.</skos:definition>
  <skos:scopeNote xml:lang="en">CaptureMethod is the method used in the initial capturing the content, not the method used to fix the content onto the carrier (c.f., FixationMethod).</skos:scopeNote>
</rdf:Description>

<!-- Property: has character -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10012">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has character</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasCharacter.en"/>
  <skos:definition xml:lang="en">A fundamental form of communication in which the content of a resource is expressed.</skos:definition>
</rdf:Description>

<!-- Property: has encoding format -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10004">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has encoding format</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasEncodingFormat.en"/>
  <skos:definition xml:lang="en">A schema, standard, etc., used to encode the content of a resource.</skos:definition>
</rdf:Description>

<!-- Property: has extension mode -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10015">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has extension mode</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionMode.en"/>
  <skos:definition xml:lang="en">A means by which the content of the resource is expected to be extended after initial release.</skos:definition>
</rdf:Description>

<!-- Property: has extension requirement -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10020">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has extension requirement</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionRequirement.en"/>
  <skos:definition xml:lang="en">An assessment of the necessity of extension of the content of the resource after initial release for its integrity.</skos:definition>
</rdf:Description>

<!-- Property: has extension termination -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10022">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has extension termination</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionTermination.en"/>
  <skos:definition xml:lang="en">An expectation as to the period of extension of the content of the resource after initial release.</skos:definition>
</rdf:Description>

<!-- Property: has fixation method -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10003">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has fixation method</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFixationMethod.en"/>
  <skos:definition xml:lang="en">A method or process by which the content of a resource is infixed or embedded in the carrier.</skos:definition>
</rdf:Description>

<!-- Property: has fixation tool -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10006">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has fixation tool</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFixationTool.en"/>
  <skos:definition xml:lang="en">A tool used to infix the content of a resource.</skos:definition>
</rdf:Description>

<!-- Property: has form or genre -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10014">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has form or genre</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFormOrGenre.en"/>
  <skos:definition xml:lang="en">A literary, musical, artistic, etc., form or genre associated with the content of a resource.</skos:definition>
</rdf:Description>

<!-- Property: has housing format -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10009">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has housing format</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasHousingFormat.en"/>
  <skos:definition xml:lang="en">The physical format of the encasing for the storage medium.</skos:definition>
  <skos:scopeNote xml:lang="en">Applies to resources conveying content on a physical carrier. Resources conveying content by means of broadcasting or online transmission will have a HousingFormat value not applicable.</skos:scopeNote>
</rdf:Description>

<!-- Property: has image dimensionality -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10024">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has image dimensionality</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasImageDimensionality.en"/>
  <skos:definition xml:lang="en">The number of spatial dimensions in which the image content of a resource is intended to be perceived.</skos:definition>
  <skos:scopeNote xml:lang="en">Applies only to content with a Character value image.</skos:scopeNote>
</rdf:Description>

<!-- Property: has image movement -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10025">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has image movement</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasImageMovement.en"/>
  <skos:definition xml:lang="en">The perceived presence or absence of movement in the image content of a resource.</skos:definition>
  <skos:scopeNote xml:lang="en">Applies only to content with a Character value image.</skos:scopeNote>
</rdf:Description>

<!-- Property: has interaction -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10011">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has interaction</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasInteraction.en"/>
  <skos:definition xml:lang="en">A capacity to respond to actions performed by the user in relation to the content of the resource.</skos:definition>
</rdf:Description>

<!-- Property: has intermediation method -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10002">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has intermediation method</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasIntermediationMethod.en"/>
  <skos:definition xml:lang="en">A method intended to be applied to the carrier to allow the content of the resource to be perceived.</skos:definition>
</rdf:Description>

<!-- Property: has intermediation tool -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10005">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has intermediation tool</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasIntermediationTool.en"/>
  <skos:definition xml:lang="en">A tool intended to be applied to the carrier to enable the content of the resource to be perceived.</skos:definition>
  <skos:scopeNote xml:lang="en">IntermediationTool identifies any physical or digital tool required to enable the resource content to be perceived as intended, including tools required to decode, decrypt or otherwise circumvent security measures.</skos:scopeNote>
</rdf:Description>

<!-- Property: has purpose -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10019">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has purpose</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasPurpose.en"/>
  <skos:definition xml:lang="en">A use for which the content of a resource is intended.</skos:definition>
</rdf:Description>

<!-- Property: has revision mode -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10016">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  
  <rdfs:label xml:lang="en">has revision mode</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasRevisionMode.en"/>
  <skos:definition xml:lang="en">A means by which the content of the resource is expected to be revised after initial release.</skos:definition>
</rdf:Description>

<!-- Property: has revision requirement -->
