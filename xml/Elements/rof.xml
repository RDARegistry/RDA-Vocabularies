  <rdfs:label xml:lang="en">Base category</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/BaseCategory.en"/>
  <skos:definition xml:lang="en">A resource category that is wholly defined in terms of primary values that are specified in the RDA/ONIX Framework.</skos:definition>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10008"/>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10009"/>
</rdf:Description>

<!-- Class: Base content category -->
