  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  
  <rdfs:label xml:lang="en">Qualified category</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedCategory.en"/>
  <skos:definition xml:lang="en">A category that qualifies a base category by the addition of a value or values that are not specified in the RDA/ONIX Framework.</skos:definition>
