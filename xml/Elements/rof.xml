  <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10005"/>
</rdf:Description>

<!-- Class: Carrier category -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10004">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
