  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/Category.en"/>
  <skos:definition xml:lang="en">A class of resources defined in terms of specific values for one or more attributes (e.g., all resources for which the value for Character is language).</skos:definition>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10003"/>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10004"/>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10005"/>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10007"/>
</rdf:Description>

<!-- Class: Content category -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10005">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10006"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  
  <rdfs:label xml:lang="en">Content category</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/ContentCategory.en"/>
  <skos:definition xml:lang="en">A class of resources defined in terms of specific values for one or more attributes of the intellectual or artistic content of a resource.</skos:definition>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10001"/>
  <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10008"/>
</rdf:Description>

<!-- Class: Qualified carrier category -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10002">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/"/>
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
  <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10007"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  
  <rdfs:label xml:lang="en">Qualified carrier category</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedCarrierCategory.en"/>
  <skos:definition xml:lang="en">A resource category that qualifies a base carrier category.</skos:definition>
