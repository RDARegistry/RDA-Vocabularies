<!-- Scheme: ROF Revision Mode -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/rofrm">
        <dc:title xml:lang="en">ROF Revision Mode</dc:title>
        <dc:description xml:lang="en">Concepts for a means by which the content of the resource is expected to be revised after initial release.</dc:description>
        <vann:preferredNamespacePrefix>rofrm</vann:preferredNamespacePrefix>
        <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-22</dc:issued>
        <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-20</dc:modified>
        <owl:versionInfo>v4.0.6</owl:versionInfo>
