  <!-- Concept: element set  -->
  <skos:Concept rdf:about="http://rdaregistry.info/termList/RDALinkedDataWork/1003">
    <skos:altLabel xml:lang="en">metadata element set</skos:altLabel>
    <skos:inScheme rdf:resource="http://rdaregistry.info/termList/RDALinkedDataWork"/>
    
    <rdakit:toolkitLabel xml:lang="en">element set</rdakit:toolkitLabel>
    <rdakit:toolkitDefinition xml:lang="en">A category of linked data work that defines classes and properties that are used in a dataset.</rdakit:toolkitDefinition>
    <skos:prefLabel xml:lang="en">element set</skos:prefLabel>
