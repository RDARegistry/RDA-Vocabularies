<!-- Concept: determinate  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofet/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofet"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6936"/>
      <skos:prefLabel xml:lang="en">determinate</skos:prefLabel>
      <skos:definition xml:lang="en">The period of extension of the content has a predetermined end.</skos:definition>
    <skos:definition xml:lang="nl">De periode van uitbreiding van de content heeft een vooraf bepaald einde.</skos:definition>
        <skos:prefLabel xml:lang="nl">eindig</skos:prefLabel>
    </skos:Concept>
<!-- Concept: indeterminate  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofet/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofet"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6937"/>
      <skos:prefLabel xml:lang="en">indeterminate</skos:prefLabel>
      <skos:definition xml:lang="en">The period of extension of the content has no predetermined end.</skos:definition>
    <skos:definition xml:lang="nl">De periode van uitbreiding van de content heeft geen vooraf bepaald einde.</skos:definition>
        <skos:prefLabel xml:lang="nl">oneindig</skos:prefLabel>
    </skos:Concept>
<!-- Concept: not applicable  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofet/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofet"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6938"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">The content is not expected to be extended.</skos:definition>
    <skos:definition xml:lang="nl">De content is niet verwacht om uitgebreid te worden.</skos:definition>
        <skos:prefLabel xml:lang="nl">niet van toepassing</skos:prefLabel>
    </skos:Concept>

