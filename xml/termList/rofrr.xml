<!-- Scheme: ROF Revision Requirement -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/rofrr">
        <dc:title>ROF Revision Requirement</dc:title>
    </skos:ConceptScheme>

<!-- Concept: essential  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrr/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrr"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6965"/>
      <skos:prefLabel xml:lang="en">essential</skos:prefLabel>
      <skos:definition xml:lang="en">Revision of the content after initial release is essential for its integrity.</skos:definition>
    </skos:Concept>
<!-- Concept: inessential  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrr/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrr"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6966"/>
      <skos:prefLabel xml:lang="en">inessential</skos:prefLabel>
      <skos:definition xml:lang="en">Revision of the content after initial release is not essential for its integrity.</skos:definition>
    </skos:Concept>
<!-- Concept: not applicable  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrr/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrr"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6967"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">The content is not expected to be revised.</skos:definition>
    </skos:Concept>

