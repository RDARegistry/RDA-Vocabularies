<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: ROF Revision Termination -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/rofrt">
        <dc:title>ROF Revision Termination</dc:title>
    </skos:ConceptScheme>

<!-- Concept: determinate  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrt/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrt"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6968"/>
      <skos:prefLabel xml:lang="en">determinate</skos:prefLabel>
      <skos:definition xml:lang="en">The period of revision of the content has a predetermined end.</skos:definition>
    </skos:Concept>
<!-- Concept: indeterminate  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrt/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrt"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6969"/>
      <skos:prefLabel xml:lang="en">indeterminate</skos:prefLabel>
      <skos:definition xml:lang="en">The period of revision of the content has no predetermined end.</skos:definition>
    </skos:Concept>
<!-- Concept: not applicable  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofrt/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofrt"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6970"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">The content is not expected to be revised.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
