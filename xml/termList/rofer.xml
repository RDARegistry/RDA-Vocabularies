<!-- Concept: essential  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofer/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofer"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6933"/>
      <skos:prefLabel xml:lang="en">essential</skos:prefLabel>
      <skos:definition xml:lang="en">Extension of the content after initial release is essential for its integrity.</skos:definition>
    </skos:Concept>
<!-- Concept: inessential  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofer/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofer"/>
