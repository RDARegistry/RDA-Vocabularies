      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6934"/>
      <skos:prefLabel xml:lang="en">inessential</skos:prefLabel>
      <skos:definition xml:lang="en">Extension of the content after initial release is not essential for its integrity.</skos:definition>
    </skos:Concept>
<!-- Concept: not applicable  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofer/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofer"/>
