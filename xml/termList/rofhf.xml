<!-- Concept: binding  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6939"/>
      <skos:prefLabel xml:lang="en">binding</skos:prefLabel>
      <skos:definition xml:lang="en">A gathering of one or more sheets in a fixed order, with or without an affixed outer cover.</skos:definition>
    </skos:Concept>
<!-- Concept: cartridge  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6942"/>
      <skos:prefLabel xml:lang="en">cartridge</skos:prefLabel>
      <skos:definition xml:lang="en">A rectangular casing fitted with a single reel holding a roll.</skos:definition>
    </skos:Concept>
<!-- Concept: cassette  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6943"/>
      <skos:prefLabel xml:lang="en">cassette</skos:prefLabel>
      <skos:definition xml:lang="en">A rectangular casing fitted with two reels holding a roll.</skos:definition>
    </skos:Concept>
<!-- Concept: flipchart  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6940"/>
      <skos:prefLabel xml:lang="en">flipchart</skos:prefLabel>
      <skos:definition xml:lang="en">A hinging device holding two or more sheets designed for use on an easel.</skos:definition>
    </skos:Concept>
<!-- Concept: not applicable  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6944"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">The storage medium is not encased in a housing.</skos:definition>
    </skos:Concept>
<!-- Concept: reel  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofhf/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofhf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6941"/>
      <skos:prefLabel xml:lang="en">reel</skos:prefLabel>
      <skos:definition xml:lang="en">A flanged spool for holding a roll.</skos:definition>
    </skos:Concept>

