<!-- Concept: interactive  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofin/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofin"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6951"/>
      <skos:prefLabel xml:lang="en">interactive</skos:prefLabel>
      <skos:definition xml:lang="en">The content responds to actions performed by the user (e.g., searches, commands, selections).</skos:definition>
    </skos:Concept>
<!-- Concept: non-interactive  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofin/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofin"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6952"/>
      <skos:prefLabel xml:lang="en">non-interactive</skos:prefLabel>
      <skos:definition xml:lang="en">The content does not respond to actions performed by the user.</skos:definition>
    </skos:Concept>

