<!-- Concept: chip  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1007" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6931"/>
      <skos:prefLabel xml:lang="en">chip</skos:prefLabel>
      <skos:definition xml:lang="en">A small wafer of semiconductor silicon.</skos:definition>
    <skos:definition xml:lang="nl">Een kleine wafel van halfgeleide siliconen.</skos:definition>
        <skos:prefLabel xml:lang="nl">chip</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">siru</skos:prefLabel>
        <skos:definition xml:lang="fi">Pieni puolijohdelevy silikonista.</skos:definition>
    <skos:prefLabel xml:lang="ar">رقاقة</skos:prefLabel>
        <skos:definition xml:lang="ar">رقاقة صغيرة من السيليكون شبه الموصل.</skos:definition>
        <skos:prefLabel xml:lang="de">Chip</skos:prefLabel>
        <skos:definition xml:lang="de">Ein kleines Plättchen aus Halbleitersilizium.</skos:definition>
    </skos:Concept>
<!-- Concept: cylinder  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6930"/>
      <skos:prefLabel xml:lang="en">cylinder</skos:prefLabel>
      <skos:definition xml:lang="en">A roller-shaped form.</skos:definition>
    <skos:definition xml:lang="nl">Een rolvormige vorm.</skos:definition>
        <skos:prefLabel xml:lang="nl">cilinder</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">sylinteri</skos:prefLabel>
        <skos:definition xml:lang="fi">Rullamainen muoto.</skos:definition>
    <skos:prefLabel xml:lang="ar">اسطوانة</skos:prefLabel>
        <skos:definition xml:lang="ar">شكل أسطواني.</skos:definition>
        <skos:prefLabel xml:lang="de">Zylinder</skos:prefLabel>
        <skos:definition xml:lang="de">Eine walzenförmige Form.</skos:definition>
    </skos:Concept>
<!-- Concept: disc  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6928"/>
      <skos:prefLabel xml:lang="en">disc</skos:prefLabel>
      <skos:definition xml:lang="en">A flat, circular piece of material (plastic, metal, etc.).</skos:definition>
    <skos:definition xml:lang="nl">Een plat, rond stuk materiaal (plastic, metaal, etc.).</skos:definition>
        <skos:prefLabel xml:lang="nl">disk</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">levy</skos:prefLabel>
        <skos:definition xml:lang="fi">Litteä, pyöreä materiaalin (muovi, metalli tms.) osa.</skos:definition>
    <skos:prefLabel xml:lang="ar">قرص</skos:prefLabel>
        <skos:definition xml:lang="ar">قطعة من مادة مسطحة دائرية (بلاستيك، معدن، إلخ).</skos:definition>
        <skos:prefLabel xml:lang="de">Disk</skos:prefLabel>
        <skos:definition xml:lang="de">Ein flaches, rundes Stück Material (Kunststoff, Metall und so weiter).</skos:definition>
    </skos:Concept>
<!-- Concept: file server  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1008" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6932"/>
      <skos:prefLabel xml:lang="en">file server</skos:prefLabel>
      <skos:definition xml:lang="en">A computer storage device with hardware and software connections to a communications network.</skos:definition>
    <skos:definition xml:lang="nl">Een computeropslagapparaat met hardware- en softwarematige verbindingen naar een communicatienetwerk.</skos:definition>
        <skos:prefLabel xml:lang="nl">bestandsserver</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">tiedostopalvelin</skos:prefLabel>
        <skos:definition xml:lang="fi">Varastoiva tietokone, jonka laitteisto ja ohjelmisto ovat yhteydessä tietoliikenneverkkoihin.</skos:definition>
    <skos:prefLabel xml:lang="ar">خادم الملفات</skos:prefLabel>
        <skos:definition xml:lang="ar">جهاز تخزين الحاسوب متصل بشبكة اتصالات من خلال أجهزة وبرامج</skos:definition>
        <skos:prefLabel xml:lang="de">File-Server</skos:prefLabel>
        <skos:definition xml:lang="de">Ein Computerspeichergerät mit Hardware- und Softwareverbindungen zu einem Kommunikationsnetz.</skos:definition>
    </skos:Concept>
<!-- Concept: roll  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6927"/>
      <skos:prefLabel xml:lang="en">roll</skos:prefLabel>
      <skos:definition xml:lang="en">A wound length of material (paper, film, tape, etc.).</skos:definition>
    <skos:definition xml:lang="nl">Een opgewonden lengte van materiaal (papier, film, tape etc.).</skos:definition>
        <skos:prefLabel xml:lang="nl">rol</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">rulla</skos:prefLabel>
        <skos:definition xml:lang="fi">Kelattu pätkä materiaalia (paperi, filmi, nauha tms.).</skos:definition>
    <skos:prefLabel xml:lang="ar">لفافة</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Rolle</skos:prefLabel>
        <skos:definition xml:lang="de">Ein aufgewickeltes Stück Material (Papier, Film, Klebeband und so weiter).</skos:definition>
    </skos:Concept>
<!-- Concept: sheet  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6925"/>
      <skos:prefLabel xml:lang="en">sheet</skos:prefLabel>
      <skos:definition xml:lang="en">A flat piece of thin material (paper, plastic, etc.), usually rectangular in shape.</skos:definition>
    <skos:definition xml:lang="nl">Een plat stuk van dun materiaal (papier, plastic etc.), meestal rechthoekig van vorm.</skos:definition>
        <skos:prefLabel xml:lang="nl">vel</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">arkki</skos:prefLabel>
        <skos:definition xml:lang="fi">Ohuen materiaalin (paperi, muovi tms.) litteä osa, yleensä nelikulmion muotoinen.</skos:definition>
    <skos:prefLabel xml:lang="ar">فرخ</skos:prefLabel>
        <skos:definition xml:lang="ar">قطعة مسطحة من مادة رقيقة (ورق، بلاستيك، إلخ)، عادة ما تكون مستطيلة الشكل.</skos:definition>
        <skos:prefLabel xml:lang="de">Blatt</skos:prefLabel>
        <skos:definition xml:lang="de">Ein flaches Stück aus dünnem Material (Papier, Kunststoff und so weiter), das in der Regel rechteckig ist.</skos:definition>
    </skos:Concept>
<!-- Concept: sphere  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6929"/>
      <skos:prefLabel xml:lang="en">sphere</skos:prefLabel>
      <skos:definition xml:lang="en">A round, ball-shaped form.</skos:definition>
    <skos:definition xml:lang="nl">Een ronde, bolvormige vorm.</skos:definition>
        <skos:prefLabel xml:lang="nl">globe</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">pallo</skos:prefLabel>
        <skos:definition xml:lang="fi">Pyöreä, pallomainen muoto.</skos:definition>
    <skos:prefLabel xml:lang="ar">جسم كروى</skos:prefLabel>
        <skos:definition xml:lang="ar">شكل دائري كروي.</skos:definition>
        <skos:prefLabel xml:lang="de">Kugel</skos:prefLabel>
        <skos:definition xml:lang="de">Eine runde, kugelförmige Form.</skos:definition>
    </skos:Concept>
<!-- Concept: strip  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofsf/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofsf"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6926"/>
      <skos:prefLabel xml:lang="en">strip</skos:prefLabel>
      <skos:definition xml:lang="en">A short length of material (paper, film, tape, etc.).</skos:definition>
    <skos:definition xml:lang="nl">Een korte lengte materiaal (papier, film, tape etc.).</skos:definition>
        <skos:prefLabel xml:lang="nl">strook</skos:prefLabel>
        <skos:prefLabel xml:lang="fi">liuska</skos:prefLabel>
        <skos:definition xml:lang="fi">Lyhyt pätkä materiaalia (paperi, filmi, nauha tms.).</skos:definition>
    <skos:prefLabel xml:lang="ar">شريط</skos:prefLabel>
        <skos:definition xml:lang="ar">مادة قصيرة الطول (ورق، فيلم، شريط، إلخ).</skos:definition>
        <skos:prefLabel xml:lang="de">Streifen</skos:prefLabel>
        <skos:definition xml:lang="de">Ein kurzes Stück Material (Papier, Film, Klebeband, und so weiter).</skos:definition>
    </skos:Concept>

