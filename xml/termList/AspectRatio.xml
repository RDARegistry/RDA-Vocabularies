<rdf:RDF xmlns:rdakit="http://metadataregistry.org/uri/profile/rdakit/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:reg="http://metadataregistry.org/uri/profile/regap/" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:dc="http://purl.org/dc/elements/1.1/">

<!-- Scheme: RDA Aspect Ratio Designation -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/AspectRatio">
        <dc:title>RDA Aspect Ratio Designation</dc:title>
    </skos:ConceptScheme>

<!-- Concept: full screen  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      
      <skos:prefLabel xml:lang="en">full screen</skos:prefLabel>
      
      <skos:prefLabel xml:lang="de">Vollbild</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">plein écran</skos:prefLabel>
      
      
      <skos:prefLabel xml:lang="es">pantalla completa</skos:prefLabel>
      
      <rdakit:toolkitLabel xml:lang="en">full screen</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="fr">plein écran</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">An aspect ratio designation for an expression of a moving image work whose ratio is less than 1.5:1.</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="fr">Désignation de format d’image pour une ressource d’images animées dont le format d’image est inférieur à 1,5:1.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="de">Vollbild</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="de">Ein Bildformat einer Bewegtbildressource, dessen Verhältnis weniger als 1,5:1 beträgt.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">pantalla completa</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="es">Proporción dimensional de un recurso de imagen en movimiento menor que 1.5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="zh">全屏</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">全屏</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="zh">动态图像资源的宽高比小于1.5：1。</rdakit:toolkitDefinition>
      <skos:altLabel xml:lang="en">full-screen</skos:altLabel>
      <skos:prefLabel xml:lang="nl">full screen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="nl">full screen</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="nl">Aspectratio voor een resource met bewegend beeld minder dan 1.5:1</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="it">a tutto schermo</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">a tutto schermo</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="fi">full screen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">full screen</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="sv">normalbild</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="sv">normalbild</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="vi">toàn màn hình</skos:prefLabel>
      <skos:altLabel xml:lang="vi">toàn-màn-hình</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="vi">toàn màn hình</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="vi">Định danh tỷ lệ về ngoài cho tài nguyên hình ảnh động nhỏ hơn 1,5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="ca">pantalla completa</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="ca">pantalla completa</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="ca">Designació de relació d’aspecte d’un recurs d’imatges en moviment la relació de la qual és inferior a 1,5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="no">fullskjerm</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">fullskjerm</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="no">En betegnelse for bildesideforhold på under 1,5:1 for en levende bilde-ressurs.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="da">fuldskærm</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">fuldskærm</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="da">Billedformat for en levende billedressource hvis ratio er mindre end 1,5:1.</rdakit:toolkitDefinition>
      
      <rdakit:toolkitDefinition xml:lang="fi">Liikkuvan kuvan kuvasuhde, joka on pienempi kuin 1.5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="el">πλήρης οθόνη</skos:prefLabel>
      <skos:altLabel xml:lang="el">πλήρης οθόνη</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="el">πλήρης οθόνη</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="el">Ένας προσδιορισμός μιας αναλογίας κάδρου για έναν πόρο κινητής εικόνας μικρότερο από 1.5:1.</rdakit:toolkitDefinition>
      <skos:notation>1001</skos:notation>
      <skos:prefLabel xml:lang="hu">standard képformátum</skos:prefLabel>
      <skos:altLabel xml:lang="hu">hagyományos képformátum</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="hu">standard képformátum</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="hu">A képméretarány megjelölése az 1,5:1-nél kisebb képméretarányú mozgóképforrások esetében.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: mixed aspect ratio  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      
      <skos:prefLabel xml:lang="en">mixed aspect ratio</skos:prefLabel>
      
      <skos:prefLabel xml:lang="de">gemischt</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">format d’image mixte</skos:prefLabel>
      
      
      <skos:prefLabel xml:lang="es">mixta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">mixed aspect ratio</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="fr">format d’image mixte</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">An aspect ratio designation for an expression of a moving image work that includes multiple aspect ratios within the same expression.</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="fr">Désignation de format d’image pour une ressource d’images animées comprenant divers formats d’image à l’intérieur de la même ressource.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="de">gemischt</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="de">Ein Bildformat einer Bewegtbildressource, die mehrere Bildformate innerhalb derselben Ressource enthält.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">mixta</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="zh">混合</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">混合</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="zh">在同一资源中动态图像资源包括多种宽高比。</rdakit:toolkitDefinition>
      
      <rdakit:toolkitDefinition xml:lang="es">Proporción dimensional de un recurso que incluye múltiples proporciones dimensionales dentro del mismo recurso.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="nl">gemengd</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="nl">gemengd</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="nl">Aspectratio voor een resource met bewegend beeld dat meerdere aspectratio's bevat binnen dezelfde resource</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="it">misto</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">misto</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="fi">yhdistelmä (kuvasuhde)</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">yhdistelmä (kuvasuhde)</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="sv">blandade bildförhållanden</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="sv">blandade bildförhållanden</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="vi">tỷ lệ bề ngoài hỗn hợp</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="vi">tỷ lệ bề ngoài hỗn hợp</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="vi">Định danh tỷ lệ về ngoài cho tài nguyên hình ảnh động bao gồm các tỷ lệ bề ngoài khác nhau bên trong cùng tài nguyên.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="ca">relació d’aspecte mixta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="ca">relació d’aspecte mixta</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="ca">Designació de relació d’aspecte d’un recurs d’imatges en moviment que inclou diverses relacions d’aspecte dins del mateix recurs.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="no">blandet bildesideforhold</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">blandet bildesideforhold</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="no">En betegnelse for bildesideforhold for en levende bilde-ressurs som har flere ulike bildesideforhold innenfor samme ressurs.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="da">blandet billedformat</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">blandet billedformat</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="da">Billedformat for en levende billedressource, der omfatter flere billedformater inden for samme ressource</rdakit:toolkitDefinition>
      
      <rdakit:toolkitDefinition xml:lang="fi">Kuvasuhde liikkuvaa kuvaa sisältävälle aineistolle, joka käsittää useita eri kuvasuhteita.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="el">μικτή αναλογία κάδρου</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="el">μικτή αναλογία κάδρου</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="el">Ένας προσδιορισμός μιας αναλογίας κάδρου για έναν πόρο κινητής εικόνας που περιλαμβάνει πολλές αναλογίες κάδρου εντός του ίδιου πόρου.</rdakit:toolkitDefinition>
      <skos:notation>1003</skos:notation>
      <skos:prefLabel xml:lang="hu">kevert képformátum</skos:prefLabel>
      <skos:altLabel xml:lang="hu">kevert képméretarány</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="hu"> kevert oldalviszony</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="hu">A képméretarány megjelölése olyan mozgóképforrások esetében, amelyek többféle képméretarányt alkalmznak egy forráson belül.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: wide screen  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/AspectRatio/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/AspectRatio"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      
      <skos:prefLabel xml:lang="en">wide screen</skos:prefLabel>
      
      <skos:prefLabel xml:lang="de">Breitbild</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">écran large</skos:prefLabel>
      
      <skos:prefLabel xml:lang="es">pantalla ancha</skos:prefLabel>
      
      <rdakit:toolkitLabel xml:lang="en">wide screen</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="fr">écran large</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">An aspect ratio designation for an expression of a moving image work whose ratio is equal to or greater than 1.5:1.</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="fr">Désignation de format d’image d’une ressource d’images animées dont le format d’image est égal ou supérieur à 1,5:1.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="de">Breitbild</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="de">Ein Bildformat einer Bewegtbildressource, dessen Verhältnis 1,5:1 oder mehr beträgt.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">pantalla ancha</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="zh">宽屏</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">宽屏</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="zh">动态图像的宽高比为1.5:1或更大。</rdakit:toolkitDefinition>
      <skos:altLabel xml:lang="en">wide-screen</skos:altLabel>
      
      <rdakit:toolkitDefinition xml:lang="es">Proporción dimensional de un recurso de imagen en movimiento de 1.5:1 o mayor.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="nl">widescreen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="nl">widescreen</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="nl">Aspectratio voor een resource met bewegend beeld van 1.5:1 of meer</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="it">a schermo panoramico</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">a schermo panoramico</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="fi">wide screen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">wide screen</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="sv">bredbild</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="sv">bredbild</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="vi">màn hình rộng</skos:prefLabel>
      <skos:altLabel xml:lang="vi">màn-hình-rộng</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="vi">màn hình rộng</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="vi">Định danh tỷ lệ về ngoài cho tài nguyên hình ảnh động là 1,5:1 hoặc lớn hơn.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="ca">pantalla panoràmica</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="ca">pantalla panoràmica</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="ca">Designació de relació d’aspecte d’un recurs d’imatges en moviment la relació de la qual és igual o superior a 1,5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="no">widescreen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">widescreen</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="no">En betegnelse for bildesideforhold på 1,5:1 eller mer for en levende bilde-ressurs.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="da">wide screen</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">wide screen</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="da">Billedformat for en levende billedressource hvis ratio er lig med eller større end 1,5:1.</rdakit:toolkitDefinition>
      
      <rdakit:toolkitDefinition xml:lang="fi">Liikkuvan kuvan kuvasuhde, joka on yhtä suuri tai suurempi kuin 1.5:1.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="el">ευρεία οθόνη</skos:prefLabel>
      <skos:altLabel xml:lang="el">ευρεία οθόνη</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="el">ευρεία οθόνη</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="el">Ένας προσδιορισμός μιας αναλογίας κάδρου για έναν πόρο κινητής εικόνας 1.5:1 ή μεγαλύτερο.</rdakit:toolkitDefinition>
      <skos:notation>1002</skos:notation>
      <skos:prefLabel xml:lang="hu">széles képformátum</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="hu">széles képformátum</rdakit:toolkitLabel>
      
      <rdakit:toolkitDefinition xml:lang="hu">A képméretarány megjelölése az 1,5:1-nél nagyobb képméretarányú mozgóképforrások esetében.</rdakit:toolkitDefinition>
    </skos:Concept>

