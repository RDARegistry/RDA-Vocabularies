        <dc:title>RDA Track Configuration</dc:title>
    </skos:ConceptScheme>

<!-- Concept: centre track  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/trackConfig/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/trackConfig"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/872"/>
      <skos:prefLabel xml:lang="en">centre track</skos:prefLabel>
      <skos:definition xml:lang="en">A track configuration in which the audio track is located in the centre of a separate film roll.</skos:definition>
      <skos:prefLabel xml:lang="de">Mittelspur</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">piste centrale</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Spurkonfiguration, bei der die Tonspur in der Mitte einer separaten Filmrolle.</skos:definition>
      <skos:prefLabel xml:lang="es">banda sonora central</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">centre track</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="fr">piste centrale</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A track configuration in which the audio track is located in the centre of a separate film roll.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">banda sonora central</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="zh">中央音轨</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">中央音轨</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">声道位于独立胶卷中心的音轨配置。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">声道位于独立胶卷中心的音轨配置。</rdakit:toolkitDefinition>
      <skos:definition xml:lang="es">Configuración de la banda sonora en la cual la pista de audio está localizada en el centro de un rollo de película separado.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="es">Configuración de la banda sonora en la cual la pista de audio está localizada en el centro de un rollo de película separado.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="sv">centralspår</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="sv">centralspår</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: edge track  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/trackConfig/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/trackConfig"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/873"/>
      <skos:prefLabel xml:lang="en">edge track</skos:prefLabel>
      <skos:definition xml:lang="en">A track configuration in which the audio track is located near the edge of a film roll.</skos:definition>
      <skos:prefLabel xml:lang="de">Seitenspur</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">piste latérale</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Spurkonfiguration, bei der die Tonspur in der Nähe des Randes einer Filmrolle liegt.</skos:definition>
      <skos:prefLabel xml:lang="es">banda sonora lateral</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">edge track</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="fr">piste latérale</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="en">A track configuration in which the audio track is located near the edge of a film roll.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">banda sonora lateral</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="zh">边缘音轨</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">边缘音轨</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">声道位于胶卷边缘的音轨配置。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">声道位于胶卷边缘的音轨配置。</rdakit:toolkitDefinition>
      <skos:definition xml:lang="es">Configuración de la banda sonora en la cual la pista de audio está localizada cerca del borde de un rollo de película.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="es">Configuración de la banda sonora en la cual la pista de audio está localizada cerca del borde de un rollo de película.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="sv">kantspår</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="sv">kantspår</rdakit:toolkitLabel>
    </skos:Concept>

