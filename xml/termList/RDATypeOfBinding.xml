<!-- Scheme: RDA Type of Binding -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/RDATypeOfBinding">
        <dc:title xml:lang="en">RDA Type Of Binding</dc:title>
        <dc:description xml:lang="en">Concepts for a method used to bind a published or unpublished manifestation.</dc:description>
        <vann:preferredNamespacePrefix>rdatb</vann:preferredNamespacePrefix>
        <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-04-04</dc:issued>
