      <skos:altLabel xml:lang="en">plans (cartography) (Deprecated)</skos:altLabel>
    </skos:Concept>
<!-- Concept: model (Deprecated)  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/extentCarto/1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/extentCarto"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1008"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/765"/>
      <skos:prefLabel xml:lang="en">model (Deprecated)</skos:prefLabel>
      <skos:definition xml:lang="en">A unit of extent of cartographic resource that is a three-dimensional representation of the whole or part of the Earth or any celestial body (real or imaginary) at any scale. (Deprecated)</skos:definition>
    </skos:Concept>
<!-- Concept: models (Deprecated)  -->
