<!-- Concept: remote-sensing image (Deprecated)  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/extentCarto/1007" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/extentCarto"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1008"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/767"/>
      <skos:prefLabel xml:lang="en">remote-sensing image (Deprecated)</skos:prefLabel>
      <skos:definition xml:lang="en">A unit of extent of cartographic resource that is a pictorial product of any remote-sensing instrument that detects and measures reflected and/or emitted electromagnetic radiation from a distance and reflected underwater sound waves in the case of sonar. (Deprecated)</skos:definition>
    </skos:Concept>
<!-- Concept: section (Deprecated)  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/extentCarto/1008" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/extentCarto"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1008"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/768"/>
      <skos:prefLabel xml:lang="en">section (Deprecated)</skos:prefLabel>
      <skos:definition xml:lang="en">A unit of extent of cartographic resource that is a scale representation of a vertical surface (commonly a plane) displaying both the profile where it intersects the surface of a celestial body, or some conceptual model, and the underlying structures along the plane of intersection (e.g., a geological section). (Deprecated)</skos:definition>
      <skos:altLabel xml:lang="en">map section (Deprecated)</skos:altLabel>
    </skos:Concept>
