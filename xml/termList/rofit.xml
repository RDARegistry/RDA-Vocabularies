<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: ROF Intermediation Tool -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/rofit">
        <dc:title>ROF Intermediation Tool</dc:title>
    </skos:ConceptScheme>

<!-- Concept: audio player  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6957"/>
      <skos:prefLabel xml:lang="en">audio player</skos:prefLabel>
      <skos:definition xml:lang="en">A device designed to play audio recordings.</skos:definition>
    </skos:Concept>
<!-- Concept: audiovisual player  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6958"/>
      <skos:prefLabel xml:lang="en">audiovisual player</skos:prefLabel>
      <skos:definition xml:lang="en">A device designed to play audiovisual recordings.</skos:definition>
    </skos:Concept>
<!-- Concept: computer  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1007" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6959"/>
      <skos:prefLabel xml:lang="en">computer</skos:prefLabel>
      <skos:definition xml:lang="en">A device designed to perform prescribed sequences of operations on electronic data.</skos:definition>
    </skos:Concept>
<!-- Concept: microform reader  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6953"/>
      <skos:prefLabel xml:lang="en">microform reader</skos:prefLabel>
      <skos:definition xml:lang="en">A device that magnifies microforms for reading with the unaided eye.</skos:definition>
    </skos:Concept>
<!-- Concept: microscope  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6954"/>
      <skos:prefLabel xml:lang="en">microscope</skos:prefLabel>
      <skos:definition xml:lang="en">An instrument that magnifies objects by means of a lens or lenses so as to reveal details invisible to the naked eye.</skos:definition>
    </skos:Concept>
<!-- Concept: not required  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1008" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6960"/>
      <skos:prefLabel xml:lang="en">not required</skos:prefLabel>
      <skos:definition xml:lang="en">No intermediation tool is required (i.e., the content of the resource is directly and adequately perceivable through one or more of the human senses).</skos:definition>
    </skos:Concept>
<!-- Concept: projector  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6955"/>
      <skos:prefLabel xml:lang="en">projector</skos:prefLabel>
      <skos:definition xml:lang="en">An optical device containing a light source and lens system for projecting an image on a screen or other surface.</skos:definition>
    </skos:Concept>
<!-- Concept: stereoscope  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/rofit/T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/rofit"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6956"/>
      <skos:prefLabel xml:lang="en">stereoscope</skos:prefLabel>
      <skos:definition xml:lang="en">An optical device with two lenses enabling each eye to see a separate image of essentially the same content to give the effect of three-dimensions.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
